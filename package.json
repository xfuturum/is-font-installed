  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.4",

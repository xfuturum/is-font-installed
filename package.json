  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@swc/core": "^1.4.0",
    "babel-jest": "29.7.0",
    "cypress": "13.6.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
